{
  "RaiseFault": {
    "DisplayName": "RF-Example",
    "FaultResponse": [
      {
        "AssignVariable": {
          "Name": "flow.var",
          "Value": 123
        }
      },
      {
        "Add": {
          "Headers": {
            "Header": {
              "_Data": "example",
              "_name": "user-agent"
            }
          }
        }
      },
      {
        "Copy": {
          "Headers": {
            "Header": {
              "_name": "header-name"
            }
          },
          "StatusCode": 304,
          "_source": "request"
        }
      },
      {
        "Remove": {
          "Headers": {
            "Header": {
              "_name": "sample-header"
            }
          }
        }
      },
      {
        "Set": {
          "Headers": {
            "Header": {
              "_Data": "{request.header.user-agent}",
              "_name": "user-agent"
            }
          },
          "Payload": {
            "_Data": "{\"name\":\"foo\", \"type\":\"bar\"}",
            "_contentType": "application/json"
          }
        }
      },
      {
        "Set": {
          "ReasonPhrase": "Server Error",
          "StatusCode": 500
        }
      }
    ],
    "IgnoreUnresolvedVariables": true,
    "ShortFaultReason": false,
    "_async": false,
    "_continueOnError": true,
    "_enabled": true,
    "_name": "RF-Example"
  }
}